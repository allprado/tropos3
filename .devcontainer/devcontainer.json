{
  "name": "Tropos3D",
  "dockerFile": "Dockerfile",
  "forwardPorts": [5173],
  "portsAttributes": {
    "5173": {
      "label": "Tropos3D Dev Server",
      "onAutoForward": "openBrowser"
    }
  },
  "postCreateCommand": "npm install",
  "customizations": {
    "vscode": {
      "extensions": [
        "bradlc.vscode-tailwindcss",
        "esbenp.prettier-vscode",
        "ms-vscode.vscode-typescript-next"
      ]
    }
  }
}
